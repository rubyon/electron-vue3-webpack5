{
  "name": "electron-vue2-webpack5",
  "version": "0.0.1",
  "description": "electron-vue2-webpack5 sample project",
  "main": "main.js",
  "repository": "https://github.com/rubyon/electron-vue3-webpack5",
  "author": "RubyOn",
  "license": "MIT",
  "scripts": {
    "dev": "node ./config/dev.js",
    "build": "node ./config/build.js",
    "clean": "node ./config/clean.js"
  },
  "dependencies": {
    "vue": "^2.6.12",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "babel-loader": "^8.2.2",
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-webpack-plugin": "^2.4.3",
    "eslint-plugin-vue": "^7.5.0",
    "prettier": "^2.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "electron": "^11.2.1",
    "electron-log": "^4.3.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-builder": "^22.9.1",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "style-loader": "^2.0.0",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "vue-eslint-parser": "^7.4.1",
    "webpack": "^5.18.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "chalk": "^4.1.0",
    "cfonts": "^2.9.1",
    "del": "^6.0.0",
    "cross-spawn": "^7.0.3"
  },
  "build": {
    "buildDependenciesFromSource": true,
    "appId": "com.rubyon.app",
    "directories": {
      "output": "build"
    },
    "linux": {
      "target": "AppImage",
      "icon": "./src/renderer/assets/icons/icon.icns"
    },
    "win": {
      "target": "portable",
      "icon": "./src/renderer/assets/icons/icon.png"
    },
    "mac": {
      "identity": null,
      "target": {
        "target": "dmg",
        "arch": "arm64"
      },
      "icon": "./src/renderer/assets/icons/icon.icns"
    },
    "files": [
      "package.json",
      {
        "from": "dist",
        "filter": [
          "renderer.js",
          "main.js",
          "index.html",
          "images/*.*"
        ]
      }
    ]
  }
}
